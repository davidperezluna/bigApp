  $('body').delegate('.load-in','click', function(e){
    e.preventDefault();
    $("#container-load-productos").removeClass("hidden");
    var url = $(this).attr('href');
    var content = $(this).data('id');
    var producto = $('#txtBusqueda').val();
    $(content).html("<div class='loading-bar determinate-center margin-top-10'></div>");
    $.ajax({
      url: url,
      method: 'get',
      data: {'producto':producto},
      success: function(data) {
        $(content).html(data);
      },
      error: function() {
        console.log('Error!');
      }
    });
  });

  $(document).keypress(function(e) {
    if(e.which == 13) {
      $('.load-in').click();
    }
  });

  $("#contrasenaRepeated").keyup(function(e){
    console.log($("#contrasena").val());
    if ($("#contrasenaRepeated").val() == $("#contrasena").val()) {
      $("#btnSubmit").removeClass("disabled");
      $(".repeated-error").removeClass("has-error");
      $(".repeated-error").addClass("has-success");
    }else{
      $("#btnSubmit").addClass("disabled");
      $(".repeated-error").addClass("has-error");
    }
  });
  $( "#input-search" ).keypress(function(e) {
    if(e.which == 13) {
      $("#contenedor").empty();
      var producto = $('#input-search').val();
      var url = "{{path('buscar_producto')}}";
      $("#contenedor").append("<div class='loading-bar determinate-center margin-top-10'></div>");
      $("#cantidadPorductos").html("");
      $.ajax({
        type: 'GET',
        data: {'nombreProducto':producto},
        url: url,
        dataType : 'json',
        success: function(data) {
          $("#cantidadPorductos").append("("+data.length+")");
          $("#contenedor").html("");
          $.each(data,function(i,item){
            var url = "{{path('show_producto_anon',{'id':'ItemId'})}}";
            url = url.replace('ItemId' , item.id);
            var img = "{{asset('/uploads/producto/'~ 'foto')}}";
            img = img.replace('foto',item.imagen);
            console.log(img);
            $("#contenedor").append("<li class='has-action-left'>"+
              "<a href='"+url+"' class='visible'>"+
              "<div class='list-action-left'>"+
              "<img src='"+img+"' class='img-radius'>"+
              "</div>"+
              "<div class='list-content'>"+
              "<span class='title'>"+item.nombre+" Valor: $"+item.valor+"</span>"+
              "<span class='caption'>Cantidad: " +item.cantidad+" Empresa: " + item.nombreEmpresa + "</span>"+
              "</div>"+
              "</a>"+
              "</li>"
              );
          });

        },
        error: function() {
          $("#contenedor").html("");
          $("#contenedor").append("Sin Resultados");
        }
      });
    }
  });

  </script>
